{
    "name": "devium/processes",
    "description": "This package is used to get a list of running processes on Windows or Unix-like systems, even Darwin",
    "keywords": [
        "ps",
        "process",
        "processes",
        "windows",
        "win",
        "unix",
        "linux",
        "macos",
        "darwin"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vano Devium",
            "email": "vano@devium.me",
            "homepage": "https://www.devium.me/"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/process": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Devium\\Processes\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7 || ^8 || ^9"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
